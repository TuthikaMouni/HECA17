package hebbal.ds;

public class StackDemo1 {
	public static void main(String[] args) {
		Stack s = new Stack();
		s.push(10);
		s.push(20);
		s.push(30);
		s.push(40);
		s.push(50);
		s.disp();
		System.out.println();
		System.out.println("peek value is : "+s.peek());
		System.out.println();
		System.out.println(s.toString());
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println();
		s.disp();
		System.out.println();
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println();
		s.disp();
		System.out.println(s.isEmpty());
	}
}
